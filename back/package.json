{
  "name": "recontact_travel_blog-back",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean:dist && babel src --out-dir dist/src && babel app.js index.js --out-dir dist",
    "clean": "yarn clean:dependencies && yarn clean:db && yarn clean:dist",
    "clean:db": "rm src/db/data.development.sqlite & exit 0",
    "clean:dependencies": "rm -rf node_modules",
    "clean:dist": "rm -rf dist",
    "lint": "./node_modules/.bin/eslint --ext .js src test",
    "lint:fix": "./node_modules/.bin/eslint --fix --ext .js src test",
    "start": "node dist/index.js",
    "start:dev": "NODE_ENV=development nodemon --exec babel-node ./index.js",
    "test": "env TZ=\"Europe/Paris\" NODE_ENV=test nyc mocha --require @babel/register --recursive",
    "test:unit": "NODE_ENV=test mocha --require @babel/register --recursive",
    "test:watch": "yarn test:unit --watch"
  },
  "dependencies": {
    "axios": "^0.21.2",
    "body-parser": "1.20.0",
    "chalk": "^4.1.0",
    "connect-history-api-fallback": "1.6.0",
    "cookie-parser": "1.4.5",
    "core-js": "^3.6.4",
    "cors": "2.8.5",
    "date-fns": "2.21.3",
    "dotenv-extended": "2.9.0",
    "dropbox": "2.5.13",
    "express": "4.17.1",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "node-mailjet": "3.3.4",
    "node-schedule": "2.0.0",
    "ramda": "0.28.0",
    "regenerator-runtime": "^0.13.7",
    "sequelize": "5.21.7"
  },
  "devDependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/node": "7.14.2",
    "@babel/plugin-transform-runtime": "7.14.3",
    "@babel/preset-env": "7.14.2",
    "@babel/register": "7.13.16",
    "@sinonjs/fake-timers": "^7.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "debug": "4.3.1",
    "dotenv": "10.0.0",
    "eslint": "8.20.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "fs": "0.0.2",
    "http": "0.0.1-security",
    "mocha": "8.4.0",
    "nodemon": "2.0.7",
    "nyc": "15.1.0",
    "pg": "8.6.0",
    "pug": "3.0.2",
    "sequelize-cli": "6.2.0",
    "serve-favicon": "2.5.0",
    "sinon": "11.1.0",
    "sinon-chai": "3.7.0",
    "sqlite3": "5.0.9",
    "supertest": "6.1.3"
  },
  "engines": {
    "yarn": ">=1.17.0",
    "node": "=16.16"
  }
}
