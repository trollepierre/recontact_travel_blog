{
  "name": "recontact_travel_blog-back",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean:dist && babel src --out-dir dist/src && babel app.js index.js --out-dir dist",
    "clean": "yarn clean:dependencies && yarn clean:db && yarn clean:dist",
    "clean:db": "rimraf src/db/data.development.sqlite",
    "clean:dependencies": "rimraf node_modules",
    "clean:dist": "rimraf dist",
    "configure": "./tools/setup/setup.sh",
    "db": "./node_modules/.bin/sequelize",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:reset": "yarn clean:db && yarn db:migrate",
    "db:rollback": "./node_modules/.bin/sequelize db:migrate:undo",
    "lint": "./node_modules/.bin/eslint --ext .js src test",
    "lint:fix": "./node_modules/.bin/eslint --fix --ext .js src test",
    "start": "node dist/index.js",
    "start:dev": "NODE_ENV=development nodemon --exec babel-node ./index.js",
    "test": "env TZ=\"Europe/Paris\" NODE_ENV=test nyc mocha --require @babel/register --recursive",
    "test:unit": "NODE_ENV=test mocha --require @babel/register --recursive",
    "test:watch": "yarn test:unit --watch"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "chalk": "^4.0.0",
    "connect-history-api-fallback": "1.6.0",
    "cookie-parser": "1.4.4",
    "core-js": "^3.6.4",
    "cors": "2.8.5",
    "date-fns": "2.9.0",
    "dotenv-extended": "2.7.1",
    "dropbox": "2.5.13",
    "express": "4.17.1",
    "lodash": "4.17.15",
    "morgan": "1.9.1",
    "node-mailjet": "3.3.1",
    "node-schedule": "1.3.2",
    "ramda": "0.27.0",
    "regenerator-runtime": "^0.13.5",
    "request": "2.88.2",
    "sequelize": "5.21.4"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/register": "7.9.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "debug": "4.1.1",
    "dotenv": "8.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.20.1",
    "fs": "0.0.2",
    "http": "0.0.1-security",
    "lolex": "6.0.0",
    "mkdirp": "1.0.3",
    "mocha": "7.0.1",
    "nodemon": "2.0.2",
    "nyc": "15.0.0",
    "pg": "7.18.2",
    "pug": "2.0.4",
    "rimraf": "3.0.2",
    "sequelize-cli": "5.5.1",
    "serve-favicon": "2.5.0",
    "sinon": "8.1.1",
    "sinon-chai": "3.4.0",
    "sqlite3": "4.1.1",
    "supertest": "4.0.2"
  },
  "engines": {
    "yarn": ">=1.17.0",
    "node": "=12"
  }
}
