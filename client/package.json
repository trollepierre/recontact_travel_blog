{
  "name": "recontact_travel_blog-front",
  "version": "1.0.0",
  "description": "Travel blog of Pierre Trollé",
  "author": "Pierre Trollé <pierre@recontact.me>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && BABEL_ENV=production webpack --config build/webpack.config.prod.js",
    "clean": "rimraf dist && mkdirp dist",
    "clean:old": "rm -rf dist node_modules test/unit/coverage",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "yarn lint --fix",
    "start": "webpack-dev-server --progress --config build/webpack.config.dev.js",
    "test": "yarn test:unit --coverage",
    "test:ci": "yarn test:unit --coverage -w 1",
    "test:unit": "env TZ=\"Europe/Paris\" jest --config config/jest.config.json",
    "test:watch": "yarn test:unit --watch"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.4",
    "@babel/runtime": "7.5.5",
    "core-js": "3.2.0",
    "date-fns": "2.0.0-alpha.36",
