    "babel-eslint": "10.0.3",
    "babel-helper-vue-jsx-merge-props": "2.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-imports": "1.5.1",
    "babel-plugin-transform-vue-jsx": "4.0.1",
