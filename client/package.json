    "vue-js-modal": "1.3.31",
    "vue-lazyload": "1.3.1",
    "vue-router": "3.1.2",
    "vue-toastr": "2.0.16",
    "webpack": "4.36.1",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-syntax-jsx": "7.2.0",
    "@babel/plugin-transform-runtime": "7.5.5",
