{
  "name": "recontact_travel_blog-front",
  "version": "1.0.0",
  "description": "Travel blog of Pierre Trollé",
  "author": "Pierre Trollé <pierre@recontact.me>",
  "license": "MIT",
  "scripts": {
    "clean": "yarn clean:dependencies && yarn clean:nuxt:files  && yarn clean:dist",
    "clean:nuxt:files": "rm -rf .nuxt",
    "clean:dependencies": "rm -rf node_modules",
    "clean:dist": "rm -rf dist",
    "dev": "PORT=3333 nuxt",
    "start": "yarn dev",
    "build": "nuxt build",
    "start-nuxt": "nuxt start",
    "generate": "nuxt generate",
    "generate:en": "NUXT_ENV_LANGUAGE=en nuxt generate",
    "generate:fr": "NUXT_ENV_LANGUAGE=fr nuxt generate",
    "lint": "eslint --ext .js,.vue .",
    "lint:fix": "yarn lint --fix",
    "precommit": "yarn lint:fix",
    "test": "env TZ=\"Europe/Paris\" jest --config config/jest.config.json",
    "test:coverage": "yarn test --coverage"
  },
  "dependencies": {
    "@babel/runtime": "7.9.6",
    "@nuxtjs/dotenv": "1.4.1",
    "axios": "0.21.0",
    "dotenv": "^8.2.0",
    "nuxt": "2.14.12",
    "vue": "^2.6.12",
    "vue-i18n": "8.22.2",
    "vue-js-modal": "2.0.0-rc.6",
    "vue-lazyload": "1.3.3",
    "vue-toastr": "2.1.2",
    "vuex": "3.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@nuxtjs/pwa": "^3.3.2",
    "@vue/test-utils": "1.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "eslint": "7.16.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-vue": "7.3.0",
    "jest": "26.6.3",
    "jest-serializer-vue": "2.0.2",
    "jsdom": "16.4.0",
    "lolex": "6.0.0",
    "typescript": "^4.1.3",
    "vue-jest": "3.0.7",
    "vue-template-compiler": "^2.6.12"
  },
  "engines": {
    "yarn": ">=1.17.0",
    "node": "=12"
  }
}
