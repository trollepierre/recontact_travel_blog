{
  "name": "recontact_travel_blog-front",
  "version": "1.0.0",
  "description": "Travel blog of Pierre Trollé",
  "author": "Pierre Trollé <pierre@recontact.me>",
  "license": "MIT",
  "scripts": {
    "clean": "yarn clean:dependencies && yarn clean:nuxt:files  && yarn clean:dist",
    "clean:nuxt:files": "rm -rf .nuxt",
    "clean:dependencies": "rm -rf node_modules",
    "clean:dist": "rm -rf dist",
    "dev": "nuxt",
    "start": "yarn dev",
    "build": "nuxt build",
    "start-nuxt": "nuxt start",
    "generate": "nuxt generate",
    "generate:en": "NUXT_ENV_LANGUAGE=en nuxt generate",
    "generate:fr": "NUXT_ENV_LANGUAGE=fr nuxt generate",
    "lint": "eslint --ext .js,.vue .",
    "lint:fix": "yarn lint --fix",
    "precommit": "yarn lint:fix",
    "test": "env TZ=\"Europe/Paris\" jest --config config/jest.config.json",
    "test:coverage": "yarn test --coverage"
  },
  "dependencies": {
    "@babel/runtime": "7.9.6",
    "@nuxtjs/dotenv": "1.4.1",
    "axios": "0.19.2",
    "nuxt": "2.12.2",
    "ramda": "0.27.0",
    "vue-analytics": "5.22.1",
    "vue-i18n": "8.15.3",
    "vue-js-modal": "1.3.31",
    "vue-lazyload": "1.3.3",
    "vue-toastr": "2.0.16",
    "vuex": "3.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@vue/test-utils": "1.0.0-beta.33",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.5.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-vue": "6.2.2",
    "jest": "25.5.3",
    "jest-serializer-vue": "2.0.2",
    "jsdom": "16.2.2",
    "lolex": "6.0.0",
    "vue-jest": "3.0.5"
  },
  "engines": {
    "yarn": ">=1.17.0",
    "node": "=12"
  }
}
