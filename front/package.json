{
  "name": "recontact_travel_blog-front",
  "version": "1.0.0",
  "description": "Travel blog of Pierre Trollé",
  "author": "Pierre Trollé <pierre@recontact.me>",
  "license": "MIT",
  "scripts": {
    "clean": "yarn clean:dependencies && yarn clean:nuxt:files  && yarn clean:dist",
    "clean:nuxt:files": "rm -rf .nuxt",
    "clean:dependencies": "rm -rf node_modules",
    "clean:dist": "rm -rf dist",
    "dev": "PORT=3333 nuxt",
    "start": "yarn dev",
    "build": "nuxt build",
    "start-nuxt": "nuxt start",
    "generate": "nuxt generate",
    "generate:en": "NUXT_ENV_LANGUAGE=en nuxt generate",
    "generate:fr": "NUXT_ENV_LANGUAGE=fr nuxt generate",
    "lint": "eslint --ext .js,.vue .",
    "lint:fix": "yarn lint --fix",
    "precommit": "yarn lint:fix",
    "test": "env TZ=\"Europe/Paris\" jest --config config/jest.config.json",
    "test:coverage": "yarn test --coverage"
  },
  "dependencies": {
    "@babel/runtime": "7.18.9",
    "@nuxtjs/dotenv": "1.4.1",
    "axios": "0.21.4",
    "axios-extensions": "^3.1.3",
    "dotenv": "^10.0.0",
    "mapbox-gl": "^2.2.0",
    "nuxt": "3.4.3",
    "vue": "^2.6.12",
    "vue-i18n": "8.27.2",
    "vue-js-modal": "2.0.0-rc.6",
    "vue-lazyload": "1.3.3",
    "vue-toastr": "2.1.2",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/style-resources": "^1.0.0",
    "@vue/test-utils": "1.3.3",
    "@vue/vue2-jest": "28",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "27.0.1",
    "eslint": "8.20.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.6.0",
    "eslint-plugin-vue": "9.3.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-serializer-vue": "2.0.2",
    "lolex": "6.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "sass-loader": "10.1.1",
    "typescript": "^5.0.2",
    "vue-template-compiler": "^2.6.12"
  },
  "engines": {
    "yarn": "1.22",
    "node": "16.16"
  }
}
