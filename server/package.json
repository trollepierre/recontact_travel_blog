{
  "name": "recontact_travel_blog-back",
  "version": "1.0.0",
  "scripts": {
    "compile": "babel src --out-dir dist/src && babel index.js --out-dir dist",
    "build": "yarn clean:dist && babel src --out-dir dist/src && babel app.js index.js --out-dir dist",
    "clean": "yarn clean:dependencies && yarn clean:db  && yarn clean:dist",
    "clean:db": "rm src/db/data.development.sqlite",
    "clean:dependencies": "rm -rf node_modules",
    "clean:dist": "rimraf dist && mkdirp dist",
    "configure": "./tools/setup/setup.sh",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "db": "./node_modules/.bin/sequelize",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:reset": "yarn clean:db && yarn db:migrate",
    "db:rollback": "./node_modules/.bin/sequelize db:migrate:undo",
    "lint": "./node_modules/.bin/eslint --ext .js src test",
    "lint:fix": "./node_modules/.bin/eslint --fix --ext .js src test",
    "start": "node dist/index.js",
    "start:dev": "nodemon --exec babel-node ./index.js",
    "test": "NODE_ENV=test nyc mocha --require @babel/register --recursive",
    "test:unit": "NODE_ENV=test mocha --require @babel/register --recursive",
    "test:watch": "NODE_ENV=test mocha --require @babel/register --recursive"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "^7.0.0",
    "body-parser": "~1.18.2",
    "connect-history-api-fallback": "^1.5.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "dotenv-extended": "^2.3.0",
    "dropbox": "^2.5.13",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "lodash": "^4.17.5",
    "mkdirp": "^0.5.1",
    "morgan": "~1.9.0",
    "node-mailjet": "^3.2.1",
    "node-schedule": "^1.3.0",
    "pg": "^7.4.1",
    "pug": "^2.0.1",
    "request": "^2.85.0",
    "rimraf": "^2.6.2",
    "sequelize": "^4.37.1",
    "sequelize-cli": "^4.0.0",
    "serve-favicon": "^2.4.5",
    "sqlite3": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-promise": "^3.7.0",
    "mocha": "^5.0.4",
    "nodemon": "^1.18.5",
    "nyc": "^12.0.2",
    "sinon": "^5.1.0",
    "sinon-chai": "^3.0.0",
    "supertest": "^3.0.0"
  },
  "engines": {
    "yarn": ">=1.10.1",
    "node": ">=10.13.0"
  }
}
