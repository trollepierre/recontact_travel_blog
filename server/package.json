{
  "name": "recontact_travel_blog-back",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean:dist && babel src --out-dir dist/src && babel app.js index.js --out-dir dist",
    "clean": "yarn clean:dependencies && yarn clean:db  && yarn clean:dist",
    "clean:db": "rm src/db/data.development.sqlite",
    "clean:dependencies": "rm -rf node_modules",
    "clean:dist": "rimraf dist && mkdirp dist",
    "configure": "./tools/setup/setup.sh",
    "db": "./node_modules/.bin/sequelize",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:reset": "yarn clean:db && yarn db:migrate",
    "db:rollback": "./node_modules/.bin/sequelize db:migrate:undo",
    "lint": "./node_modules/.bin/eslint --ext .js src test",
    "lint:fix": "./node_modules/.bin/eslint --fix --ext .js src test",
    "start": "node dist/index.js",
    "start:dev": "nodemon --exec babel-node ./index.js",
    "test": "NODE_ENV=test nyc mocha --require @babel/register --recursive",
    "test:unit": "NODE_ENV=test mocha --require @babel/register --recursive",
    "test:watch": "yarn test:unit --watch"
  },
  "dependencies": {
    "connect-history-api-fallback": "1.6.0",
    "dotenv-extended": "2.4.0",
    "dropbox": "2.5.13",
    "express": "4.17.1",
    "lodash": "4.17.11",
    "node-mailjet": "3.3.1",
    "node-schedule": "1.3.2",
    "ramda": "0.26.1",
    "request": "2.88.0",
    "sequelize": "5.8.9"
  },
  "devDependencies": {
    "@babel/polyfill": "7.4.4",
    "@babel/register": "7.4.4",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "debug": "4.1.1",
    "dotenv": "8.0.0",
    "fs": "0.0.2",
    "http": "0.0.0",
    "mkdirp": "0.5.1",
    "morgan": "1.9.1",
    "pg": "7.11.0",
    "pug": "2.0.3",
    "rimraf": "2.6.3",
    "sequelize-cli": "5.5.0",
    "serve-favicon": "2.5.0",
    "sqlite3": "4.0.9",
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/node": "7.4.5",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.3",
    "lolex": "4.1.0",
    "mocha": "6.1.4",
    "nodemon": "1.19.1",
    "nyc": "14.1.1",
    "sinon": "7.3.2",
    "sinon-chai": "3.3.0",
    "supertest": "4.0.2"
  },
  "engines": {
    "yarn": ">=1.13.0",
    "node": "10.16.0"
  }
}
