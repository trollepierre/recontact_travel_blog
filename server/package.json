{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "clean": "rm -rf node_modules",
    "lint": "./node_modules/.bin/eslint --ext .js src test",
    "lint:fix": "./node_modules/.bin/eslint --fix --ext .js src test",
    "test": "NODE_ENV=test nyc mocha --recursive",
    "test:watch": "NODE_ENV=test ./node_modules/mocha/bin/mocha --recursive --watch",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "dropbox": "^2.5.7",
    "express": "~4.15.2",
    "morgan": "~1.8.1",
    "pug": "~2.0.0-beta11",
    "request": "^2.81.0",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-promise": "^3.5.0",
    "mocha": "^3.4.2",
    "nyc": "^11.1.0",
    "sinon": "^2.4.1",
    "sinon-chai": "^2.12.0",
    "supertest": "^3.0.0"
  }
}
