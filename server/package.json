{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "npm run clean:dependencies && npm run clean:db",
    "clean:dependencies": "rm -rf node_modules",
    "clean:db": "rm src/db/data.development.sqlite",
    "configure": "./tools/setup/setup.sh",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "db": "./node_modules/.bin/sequelize",
    "db:help": "./node_modules/.bin/sequelize help",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:reset": "npm run clean:db && npm run db:migrate",
    "db:rollback": "./node_modules/.bin/sequelize db:migrate:undo",
    "lint": "./node_modules/.bin/eslint --ext .js src test",
    "lint:fix": "./node_modules/.bin/eslint --fix --ext .js src test",
    "start": "node ./bin/www",
    "start:dev": "nodemon ./bin/www",
    "test": "NODE_ENV=test nyc mocha --recursive",
    "test:watch": "NODE_ENV=test ./node_modules/mocha/bin/mocha --recursive --watch"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "^2.6.9",
    "dotenv": "^4.0.0",
    "dropbox": "^2.5.13",
    "express": "^4.15.5",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "lodash": "^4.17.5",
    "morgan": "~1.8.1",
    "node-mailjet": "^3.2.1",
    "node-schedule": "^1.3.0",
    "pg": "^7.4.1",
    "pug": "^2.0.1",
    "request": "^2.85.0",
    "sequelize": "^4.37.1",
    "sequelize-cli": "^3.2.0",
    "serve-favicon": "^2.4.5",
    "sqlite3": "^3.1.13"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.19.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-promise": "^3.7.0",
    "mocha": "^3.5.3",
    "nodemon": "^1.17.2",
    "nyc": "^11.6.0",
    "sinon": "^2.4.1",
    "sinon-chai": "^2.14.0",
    "supertest": "^3.0.0"
  }
}
